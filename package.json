    "@types/react-dom": "^17.0.11",
    "@types/react-router": "^5.1.17",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "connected-react-router": "^6.9.2",
    "history": "^5.1.0",
    "immer": "^9.0.7",
    "react": "^17.0.2",
